<p id="notice"><%= notice %></p>

<h1>Tarents</h1>

<table>
  <thead>
    <tr>
      <th>Learning level</th>
      <th>User</th>
      <th>Skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tarents.each do |tarent| %>
      <tr>
        <td><%= tarent.learning_level %></td>
        <td><%= tarent.user %></td>
        <td><%= tarent.skill %></td>
        <td><%= link_to 'Show', tarent %></td>
        <td><%= link_to 'Edit', edit_tarent_path(tarent) %></td>
        <td><%= link_to 'Destroy', tarent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tarent', new_tarent_path %>
